; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Bin Day"
run/main_scene="res://maps/BinRoninIsland/bin_ronin_island.tscn"
config/features=PackedStringArray("4.4", "Forward Plus")
run/max_fps=60
config/icon="res://icon.svg"

[autoload]

Game="*res://scripts/Game.gd"
PropRespawnManager="*res://scripts/prop_respawn_manager.gd"
TrafficManager="*res://traffic/traffic_manager.gd"
PedestrianManager="*res://npcs/pedestrian_manager.gd"
PauseAndHud="*res://menu/pause_and_hud.tscn"

[debug]

file_logging/enable_file_logging=true
file_logging/max_log_files=10

[dialogic]

directories/dch_directory={
"junktender": "res://story/characters/junktender.dch"
}
directories/dtl_directory={
"junktender_dialogue": "res://story/junktender_dialogue.dtl"
}
variables={
"has_met_junktender": false
}
extensions_folder="res://addons/dialogic_additions"
text/letter_speed=0.02
text/initial_text_reveal_skippable=true
text/text_reveal_skip_delay=0.1
text/advance_delay=0.1
text/autoadvance_per_character_delay=0.1
text/autoadvance_ignored_characters_enabled=true
audio/max_channels=4.0
animations/join_default_length=0.5
animations/join_default_wait=true
animations/leave_default_length=0.5
animations/leave_default_wait=true
animations/cross_fade_default_length=0.5
choices/autofocus_first=true
choices/delay=0.2
choices/reveal_delay=0
choices/reveal_by_input=false
save/autosave_delay=60.0
save/encryption_on_exports_only=true
text/autopauses={}
glossary/default_case_sensitive=true
layout/style_list=["res://ui/dialogic styles/npc_dialogue_tree.tres"]
layout/default_style="res://ui/dialogic styles/npc_dialogue_tree.tres"
timer/process_in_physics=true
text/autoadvance_fixed_delay=0.0

[display]

window/size/viewport_width=3840
window/size/viewport_height=2160
window/size/mode=2
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"

[dotnet]

project/assembly_name="Fast Traffic"

[editor_plugins]

enabled=PackedStringArray("res://addons/boujie_water_shader/plugin.cfg", "res://addons/inkgd/plugin.cfg", "res://addons/proton_scatter/plugin.cfg", "res://addons/road-generator/plugin.cfg", "res://addons/terrain_3d/plugin.cfg")

[global_group]

Road="Vehicles will treat this surface as a road"
Grass="Vehicles will treat this surface as grass"
ExplosionCatcher="Used to fine-tune how explosions push larger objects"
Building=""

[inkgd]

register_templates=true

